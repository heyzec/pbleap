{
  "name": "server",
  "version": "0.0.1",
  "main": "main.js",
  "scripts": {
    "compile:wrapper": "tsc src/wrapper.ts --outDir dist --module commonjs --target ES2020",
    "compile:extension": "webpack --mode production --devtool hidden-source-map",
    "build:wasm": "cp parsers/* dist/",
    "build": "npm run compile:extension",
    "format": "prettier . --write",
    "watch": "webpack --watch",
    "vscode:prepublish": "npm run build",
    "package": "vsce package --no-dependencies"
  },
  "description": "",
  "dependencies": {
    "vscode-languageserver": "^9.0.1",
    "vscode-languageserver-textdocument": "^1.0.12",
    "web-tree-sitter": "^0.25.10"
  },
  "devDependencies": {
    "ts-loader": "^9.5.4",
    "typescript": "^5.9.3",
    "webpack": "^5.103.0",
    "webpack-cli": "^6.0.1"
  }
}
