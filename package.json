{
  "name": "vscode-ext",
  "version": "0.0.1",
  "publisher": "templates",
  "icon": "assets/icon.png",
  "engines": {
    "vscode": "^1.93.0"
  },
  "main": "./dist/extension.js",
  "activationEvents": [
    "*"
  ],
  "contributes": {
    "commands": [
      {
        "command": "vscode-ext.helloWorld",
        "title": "Hello World"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "webpack",
    "watch": "webpack --watch",
    "compile": "webpack --mode production --devtool hidden-source-map",
    "package": "vsce package --no-dependencies --allow-missing-repository --allow-star-activation"
  },
  "devDependencies": {
    "@types/node": "^22.5.4",
    "@types/vscode": "^1.93.0",
    "@vscode/vsce": "^3.6.2",
    "esbuild": "^0.25.12",
    "esbuild-plugin-polyfill-node": "^0.3.0",
    "typescript": "^5.6.2"
  },
  "dependencies": {
    "tree-sitter-go": "^0.23.4",
    "web-tree-sitter": "^0.25.10"
  }
}
